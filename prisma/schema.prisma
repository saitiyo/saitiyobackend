generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "mongodb"
  url = env("DATABASE_URL")
}


model admin {
    id                 String @id @default(auto()) @map("_id") @db.ObjectId
    firstName          String
    lastName           String
    email              String
    password           String
    createdAt          DateTime           @default(now())
    updatedAt          DateTime           @updatedAt  @default(now())
}

model user {
    id                 String @id @default(auto()) @map("_id") @db.ObjectId
    firstName          String
    lastName           String
    email              String
    password           String
    createdAt          DateTime           @default(now())
    updatedAt          DateTime           @updatedAt  @default(now())
}

